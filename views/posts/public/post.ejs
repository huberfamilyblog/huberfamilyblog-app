<%- include("../../_header") -%>

<div class="container-fluid">
    <div class="d-flex p-2 justify-content-between flex-wrap-reverse">
        <div>
            <h2 class="blog-post-title"><%= post.title %> </h2>
            <p><%= post.content %> </p>
        </div>
        <div>
            <img src="/<%= post.img_path %>" class="img-fluid" alt="Blog post image" style="width: 400px;" />
        </div>
    </div>
</div>